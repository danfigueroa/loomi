# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/transactions_db?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# Server
PORT=3002
NODE_ENV=development

# JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# API
API_VERSION=v1
API_PREFIX=/api

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# External Services
CUSTOMERS_SERVICE_URL=http://localhost:3001