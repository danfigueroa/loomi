{
  "name": "loomi-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Loomi microservices",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:up": "docker-compose -f docker-compose.test.yml up -d --build",
    "docker:down": "docker-compose -f docker-compose.test.yml down -v",
    "docker:logs": "docker-compose -f docker-compose.test.yml logs -f"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "jsonwebtoken": "^9.0.2"
  }
}