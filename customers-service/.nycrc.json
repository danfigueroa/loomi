{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/types/**",
    "dist/**",
    "coverage/**",
    "node_modules/**",
    "prisma/**"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}